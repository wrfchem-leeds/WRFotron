&share
 wrf_core         = 'ARW',
 max_dom          = 1,
 start_date       = '__startDate__','__startDate__',
 end_date         = '__endDate__','__endDate__',
 interval_seconds = __metIncSec__,
 io_form_geogrid  = 2,
/

&geogrid
 parent_id         =   1, 1,
 parent_grid_ratio =   1, 3,
 i_parent_start    =   1, 35,
 j_parent_start    =   1, 10,
 e_we              =  140, 220,
 e_sn              =  140, 120,
 geog_data_res     = 'default', 'default',
 dx = 30000,
 dy = 30000,
 map_proj = 'lambert',
 ref_lat   =  20.0,
 ref_lon   =  80.0,
 truelat1  =  10.0,
 truelat2  =  30.0,
 stand_lon =  80.0,
 geog_data_path = '__geogDir__'
/

&ungrib
 out_format = 'WPS',
 prefix = 'FILE',
/

&metgrid
 fg_name = 'FILE',
 io_form_metgrid = 2,
/
